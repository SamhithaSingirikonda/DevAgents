        y = row * self.cell_size + self.cell_size // 2
        color = "black" if player == 1 else "white"
        radius = self.cell_size // 3
        self.canvas.create_oval(x-radius, y-radius, x+radius, y+radius, 
                               fill=color, outline="black")

